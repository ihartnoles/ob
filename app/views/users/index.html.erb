<div class="container">
    <div class="row">
<div class="row">
      <article class="col-sm-10 col-md-10 content">
    <div class="my-account margin-top">
      <div class="title-box">
           <header>
             <h3 class="title">Onboarding System Users</h3>
         </header>
        </div>
        <div class="table-responsive">

      <table id="usertable" class="table table-responsive table-hover table-condensed"> 
        <thead>
        <tr>
          <th>user</th>
          <th>access level</th>
          <th>module</th>
          <th>actions</th>
        </tr>
      </thead>
       <% @users.each do |user| %>
        <tr>
          <td><%= user.netid %></td>
          <td><%= user.access_level %></td>
          <td><%= user.module %></td>
          <td><%#= link_to 'Show', user %><%= link_to 'Edit', edit_user_path(user) %> | <%= link_to "Delete", user_path(user), :method => :delete %>
            <%#= link_to 'Destroy', user, method: :delete, data: { confirm: 'Are you sure?' } %></td>
        </tr>
      <% end %>
      </table>
        </div><!-- .responsive -->
      </div>
     </article><!-- .content -->
   </div>
</div>
<br />
<%= link_to 'New User', new_user_path %>

<script src="https://ajax.googleapis.com/ajax/libs/jquery/1.11.0/jquery.min.js"></script>
<script type="text/javascript" language="javascript" src="//cdn.datatables.net/1.10.7/js/jquery.dataTables.min.js"></script>
<script type="text/javascript" language="javascript" src="//cdn.datatables.net/plug-ins/1.10.7/integration/bootstrap/3/dataTables.bootstrap.js"></script> 

<script type="text/javascript">
 $(document).ready(function() {
    $('#usertable').DataTable();
 });
</script>