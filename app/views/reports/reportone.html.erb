<!-- 
<h1>Reports#reportseven</h1>
<p>Find me in app/views/reports/reportseven.html.erb</p> 
-->


<div class="row">
<div class="col-md-12">
            
            <div class="box border primary">
                <div class="box-title">
                    <h4><i class="fa fa-table"></i>Report</h4>
                </div>
                <div class="box-body">

               <table id="fticreport" cellpadding="0" cellspacing="0" border="0" class="datatable table table-condensed table-striped table-bordered table-hover">
                     <thead>
                         <tr>
						    <th>znum</th>
						    <th>netid</th>
						    <th>last name</th>
						    <th>firstname</th>
						    <th>year</th>
						    <th class="hidden-xs">term</th>
						    <th class="hidden-xs">email</th> 
						    <th>deposit</th>             
						    <th>immunization</th>   
						    <th>residency</th>   
						    <th>aleks</th>
						    <th>orientation</th>    
						    <th class="hidden-xs">emerg. contacts</th>
						    <th class="hidden-xs">fau alert</th>
						  </tr>
                    </thead>
                    

                    <tbody>
		                   <% @students.each do |s| %>
                         		<tr>                              
                                	<td><%= s['z_number'] %></td>
                                	<td><%= s['gobtpac_external_user'] %></td>
                                	<td><%= s['l_name'] %></td>
                                	<td><%= s['f_name'] %></td>
                                	<td><%= s['year'] %></td>
                                	<td><%= s['term'] %></td>
							    	<td><%= s['goremal_email_address'] %></td>
							    	<td><%= s['deposit_status'] %></td>
							    	<td><%= s['immunization_status'] %></td>
							    	<td><%= s['residency_status'] %></td>
							    	<td><%= s['aleks_status'] %></td>
							    	<td><%= s['orientation_status'] %></td>
							    	<td><%= s['fau_alert_status'] %></td>
                               </tr>                            
		                  <% end %>
		             </tbody>

		             <tfoot>
                         <tr>
							<th>znum</th>
						    <th>netid</th>
						    <th>last name</th>
						    <th>firstname</th>
						    <th>year</th>
						    <th class="hidden-xs">term</th>
						    <th class="hidden-xs">email</th> 
						    <th>deposit</th>             
						    <th>immunization</th>   
						    <th>residency</th>   
						    <th>aleks</th>
						    <th>orientation</th>    
						    <th class="hidden-xs">emerg. contacts</th>
						    <th class="hidden-xs">fau alert</th>
						  </tr>
                    	</tfoot>
		            </table>

		             Download: <%#= link_to "Excel", reports_reportseven_path(format: "xls") %>
                    <br />

		        </div>
		     </div>
  </div>
</div>		


<!-- JQUERY -->
<%#= render 'shared/javascript' %>

<script type="text/javascript">
$(document).ready(function(){
	 $('#fticreport').dataTable();
}
</script>

