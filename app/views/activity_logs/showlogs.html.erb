	 <p>&nbsp;</p>

	  <div class="row">
      <article class="col-sm-10 col-md-10 content">
		<div class="my-account margin-top">
			<div class="title-box">
	         <header>
	           <h3 class="title"><%= @table_label.upcase %></h3>
	    	 </header>
	      </div>
	      <div class="table-responsive">
			<table id="logtable" class="table table-striped table-hover text-left my-orders-table">
             <thead>
	          <tr >
	            <th>Netid</th>
	            <th>Browser</th>
	            <th>IP</th>
	            <th>Note</th>
	            <th>Created</th>	                                
	          </tr>
	        </thead>
       		 <tbody>	
       		 	 <% @logs.each do |lg| %>
		             <tr>
		              <td><%= lg.netid %></td>
			              <td><%= lg.browser %></td>
			              <td><%= lg.ip_address %></td>
			              <td><%= lg.note %></td>
			              <td class="text-info"><em><%= lg.created_at %></em></td>			              
		              </tr>
		         <% end %>
             </tbody>
      	</table>

         </div><!-- .responsive -->
		</div>
      </article><!-- .content -->
	  
	
    </div>
  </div>

