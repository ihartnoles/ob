<%= javascript_tag do %>

  $("#btnNetID").click(function() {    
   
  <% if request.server_name.include?('10.16.10.141') %>
     window.location.replace("http://10.16.10.141:3000/home");
  <% else %>
     window.location.replace("https://obdev.fau.edu/home");      
  <% end %>
   
  });
  
  $("#btnClaimNetID").click(function() {    
     <% if request.server_name.include?('owldone') %>
     window.open("http://accounts.fau.edu/", "PopupWindow", "width=600,height=600,scrollbars=yes,resizable=no");
    <% else %>
     window.location.replace("https://accountst.fau.edu/", "PopupWindow", "width=600,height=600,scrollbars=yes,resizable=no");      
    <% end %>
  });

<% end %>